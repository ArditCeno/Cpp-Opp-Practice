#include <iostream>
#include <string>
using namespace std;

class Person{
    public:
     string name;
     int age;
     void printo() {
         cout << name << " " << age << "\n";
     }
};
class Student : public Person{
    public: 
    int grade;
    void printo(){
            cout << grade << " " << name << " " << age << "\n";
        }
    };
    int main(){
        Student s;
        s.name="Ardit";
          s.age = 20;
          s.grade = 10;
          s.printo();
    }
